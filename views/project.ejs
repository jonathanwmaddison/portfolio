<%- include('header',{back: "active"}) %>

<div class="title-panel">
<h1 class="project-title text-center">
<%=projectData.title %>
</h1>
</div>
<div class="container-fluid">
<div class="row">
    <div class="col-md-6">
       <img class="img-responsive" src=<%= projectData.image %>>
       <div class="text-center">
           <a class="btn btn-primary  project-link" target="blank" href= <%=  projectData.webpage+"" %>>Visit <%=projectData.title %></a>
           <%if (projectData.github) { %>
             <a class="btn btn-primary  project-link" target="blank" href=<%= projectData.github+"" %>>View Code</a>
            <% } %>
       </div>
    </div>
    <div class="col-md-6">
        <h3>Summary</h3>
        <p><%=projectData.summary %> </p>
        <ul class="user-stories">
            <% projectData.userStories.forEach(function(story) { %>
                <li> <%= story %> </li>
            <% }) %>
        </ul>
        <h4> This project incorporated the following tools </h4>
       <ul class="technologies-used">
            <% projectData.tools.forEach(function(tool) { %>
                <li> <%= tool %> </li>
            <% }) %>
       </ul>
    </div>
</div>

</div>
<% include footer %>
